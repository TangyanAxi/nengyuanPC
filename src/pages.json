{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^tm-(.*)": "@/tmui/components/tm-$1/tm-$1.vue",
      "^uni-(.*)": "@/components/uni-ui/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/MerchantTransactions/index",
      "style": {
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/login/index",
      "style": {
        "navigationStyle": "custom"
      }
    }
  ],
  "globalStyle": {
    "navigationStyle": "custom",
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#909191",
    "selectedColor": "#247CF7",
    "backgroundColor": "#FFFFFF",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "static/images/tabBar/home.png",
        "selectedIconPath": "static/images/tabBar/selectedHome.png"
      },
      {
        "pagePath": "pages/knowledge/index",
        "text": "知识",
        "iconPath": "static/images/tabBar/knowledge.png",
        "selectedIconPath": "static/images/tabBar/selectedKnowledge.png"
      }
    ]
  }
}
